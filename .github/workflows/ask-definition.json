{
    "family": "react-run-task",
    "executionRoleArn": "{EXECUTION_ROLE_ARN}",
    "containerDefinitions": [
      {
        "name": "react-app",
        "image": "${ECR_REPOSITORY}:${IMAGE_TAG}",
        "memory": 512,
        "cpu": 256,
        "essential": true,
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80
          }
        ]
      }
    ],
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512"
  }
  